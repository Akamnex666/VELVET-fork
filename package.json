{
  "name": "velvet",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 8080 --turbopack",
    "build": "next build",
    "start": "next start -p 8080",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --coverage --watchAll=false",
    "test:components": "jest --testPathPattern=components",
    "test:hooks": "jest --testPathPattern=hooks",
    "test:utils": "jest --testPathPattern=utils",
    "test:pages": "jest --testPathPattern=pages",
    "test:report": "node scripts/test-reporter.js",
    "test:clear": "jest --clearCache",
    "test:open": "npm run test:coverage && start coverage/lcov-report/index.html"
  },
  "dependencies": {
    "@stripe/react-stripe-js": "^3.9.0",
    "@stripe/stripe-js": "^7.8.0",
    "@supabase/supabase-js": "^2.52.1",
    "next": "15.4.4",
    "react": "18.3.0",
    "react-dom": "18.3.0",
    "stripe": "^18.4.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^15.0.7",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.8",
    "@types/node": "^20",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.0.11",
    "babel-jest": "^29.7.0",
    "eslint": "^9",
    "eslint-config-next": "15.4.4",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
